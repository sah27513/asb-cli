{
  "name": "asb-cli",
  "version": "0.0.1",
  "description": "Asure Service Bus CLI client",
  "main": "lib/index.js",
  "bin": {
    "asb": "./bin/asb.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest -u",
    "build": "backpack build",
    "start": "backpack"
  },
  "keywords": [
    "Azure Service Bus",
    "Service Bus"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sah27513/asb-cli.git"
  },
  "jest": {
    "moduleNameMapper": {
      "^asb": "<rootDir>/src/index.js",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "preferGlobal": true,
  "author": "Stephen Halliburton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sah27513/asb-cli/issues"
  },
  "homepage": "https://github.com/sah27513/asb-cli#readme",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "azure": "^2.3.1-preview",
    "babel-jest": "^24.9.0",
    "backpack-core": "^0.8.4",
    "chalk": "^2.4.2",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "jest": "^24.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0"
  }
}
